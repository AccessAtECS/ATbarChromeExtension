{
	"manifest_version": 2,
	"name": "ATbar",
	"version": "2.4",
	"author": "ECS Accessibility Team",
	"description": "Accessibility toolbar",
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"name": "Start ATbar on this page",
		"default_icon": "icon48.png",
		"icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" }
	},
	"permissions": [
		"tabs",
		"*://*/*"
	]
}